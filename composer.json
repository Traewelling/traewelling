{
    "name": "traewelling/traewelling",
    "type": "project",
    "description": "Free and open source check-in service to log your public transit journeys",
    "keywords": [
        "traewelling",
        "tr√§welling",
        "social",
        "network",
        "trains",
        "public transport"
    ],
    "license": "AGPL-3.0-only",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-json": "*",
        "abraham/twitteroauth": "^3.0",
        "barryvdh/laravel-dompdf": "^0.9",
        "brianium/paratest": "^6.3",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.1",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.71",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "revolution/laravel-mastodon-api": "^2.0",
        "spatie/icalendar-generator": "^2.0",
        "spatie/laravel-sitemap": "^6.0",
        "trwl/socialite-mastodon": "^1.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-debugbar": "^3.5",
        "beyondcode/laravel-dump-server": "^1.3",
        "facade/ignition": "^2.3.6",
        "filp/whoops": "^2.0",
        "fakerphp/faker": "^1.9",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
