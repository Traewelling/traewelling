{
    "name": "traewelling/traewelling",
    "type": "project",
    "description": "Free and open source check-in service to log your public transit journeys",
    "keywords": [
        "traewelling",
        "tr√§welling",
        "social",
        "network",
        "trains",
        "public transport"
    ],
    "license": "AGPL-3.0-only",
    "require": {
        "php": "^8.1",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-sodium": "*",
        "abraham/twitteroauth": "^4.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "brianium/paratest": "^6.3",
        "coderjerk/bird-elephant": "^0.5.0",
        "darkaonline/l5-swagger": "^8.3",
        "doctrine/dbal": "^3.1.4",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.1",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^9.0",
        "laravel/passport": "^11.2",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "mrkriskrisu/db-wagenreihung-php": "0.2",
        "revolution/laravel-mastodon-api": "^2.0",
        "romanzipp/laravel-queue-monitor": "^2.3",
        "smolblog/oauth2-twitter": "^1.0",
        "spatie/icalendar-generator": "^2.0",
        "spatie/laravel-sitemap": "^6.0",
        "spatie/laravel-validation-rules": "^3.2",
        "spatie/laravel-webhook-server": "^3.4",
        "trwl/socialite-mastodon": "^1.2",
        "uwumarie/laravel-bitflags": "dev-main"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "beyondcode/laravel-dump-server": "dev-master",
        "ext-pdo_sqlite": "*",
        "fakerphp/faker": "^1.9",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/NyCodeGHG/laravel-bitflags"
        }
    ]
}
