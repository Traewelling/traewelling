{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "lib": [
            "es2016",
            "es2015",
            "dom"
        ]
    },
    "include": [
        "resources/js/**/*",
        "resources/ts/**/*",
        "resources/types/**/*",
        "resources/vue/**/*",
    ]
}
